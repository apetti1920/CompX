{
  "name": "@compx/electron_app",
  "version": "1.0.0",
  "description": "Electron",
  "main": "./dist/main/electron_app/src/index.js",
  "scripts": {
    "prebuild": "rimraf dist/main",
    "build": "npx tsc",
    "start": "electron .",
    "builder": "electron-builder",
    "postbuilder": "electron-builder install-app-deps"
  },
  "build": {
    "appId": "com.inflow.compx",
    "productName": "CompX",
    "nsis": {
      "artifactName": "CompX.setup.${version}.${ext}",
      "uninstallDisplayName": "CompX"
    },
    "files": [
      "dist/**/*",
      "package.json"
    ],
    "mac": {
      "target": "dmg"
    }
  },
  "keywords": [],
  "author": "",
  "license": "ISC",
  "devDependencies": {
    "electron": "^38.4.0",
    "electron-builder": "^25.1.8"
  },
  "dependencies": {
    "@compx/common": "1.0.0",
    "@compx/electron_loader": "^1.0.0",
    "@compx/web_app": "^1.0.0",
    "@types/tmp": "^0.2.3",
    "loglevel": "^1.8.1",
    "tmp": "^0.2.1",
    "uuid": "^8.3.2"
  }
}
