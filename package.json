{
  "name": "compx",
  "private": true,
  "version": "1.0.0",
  "imports": {
    "assets/*": "assets/*"
  },
  "scripts": {
    "build:electron": "cross-env-shell BUILD_TYPE=\"electron\" \"pushd packages/electron_app && npm run build && popd\"",
    "start:electron": "npm run build:electron && npx lerna run start --scope=@compx/electron_app",
    "start:web": "BUILD_TYPE=\"web\" npx lerna run --scope=@compx/web_app start",
    "test": "jest",
    "pretest:pretty": "npx prettier --write --ignore-path .gitignore $(git diff --name-only HEAD | grep -E '\\.(js|jsx|ts|tsx)$' | xargs)",
    "prepare": "husky install"
  },
  "keywords": [],
  "author": "Aidan Petti",
  "license": "ISC",
  "devDependencies": {
    "@types/jest": "^29.0.3",
    "@types/lodash": "^4.14.178",
    "@types/node": "^18.7.18",
    "@typescript-eslint/eslint-plugin": "^5.37.0",
    "@typescript-eslint/parser": "^5.37.0",
    "cross-env": "^7.0.3",
    "dotenv-webpack": "^8.0.1",
    "eslint": "^8.23.1",
    "eslint-config-airbnb": "^19.0.4",
    "eslint-config-node": "^4.1.0",
    "eslint-config-prettier": "^8.5.0",
    "eslint-import-resolver-typescript": "^3.5.1",
    "eslint-plugin-import": "^2.26.0",
    "eslint-plugin-jsx-a11y": "^6.6.1",
    "eslint-plugin-node": "^11.1.0",
    "eslint-plugin-prettier": "^4.2.1",
    "eslint-plugin-react": "^7.31.8",
    "eslint-plugin-react-hooks": "^4.6.0",
    "husky": "^8.0.1",
    "jest": "^29.0.3",
    "lint-staged": "^13.0.3",
    "ncp": "^2.0.0",
    "pre-commit": "^1.2.2",
    "prettier": "^2.7.1",
    "rimraf": "^3.0.2",
    "ts-jest": "^29.0.1",
    "ts-node": "^10.4.0",
    "typescript": "^4.4.4"
  },
  "dependencies": {
    "lodash": "^4.17.21"
  }
}
